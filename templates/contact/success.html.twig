{% extends 'base.html.twig' %}
{% block body %}
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Formulaire envoyé avec succès"/>
    <title> {% block title %} Envoyé {% endblock %} </title>
</head>

<body>

    <div class="flex_main">

        <form method="post" class="position_change">
            <h2 class="alert alert-success"> Votre demande a été envoyé avec succès !</h2>
            <a class="btn btn-light" href="{{ path('app_login') }}">Précédent</a>
        </form>
    </div>
</body>
</html>

{% endblock %}